<?xml version='1.0' encoding='utf-8'?>
<widget id="com.subzerodigital.copsNotes" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>copsNotes</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="subzerodigital@gamil.com" href="">
        Stanley Yinan Yang
    </author>
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
    </feature>
</widget>
